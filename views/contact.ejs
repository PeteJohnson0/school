        <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.3/js/materialize.min.js"></script>
    </head>
    <body>
      <div class="container">
        <nav>
            <div class="nav-wrapper">
            <a href="../../../index.html" class="brand-logo">New School Cool</a>
            <ul id="nav-mobile" class="right hide-on-med-and-down">
                <li><a href="/courses">Courses</a></li>
                <li><a href="/contact">Contact Us</a></li>
            </ul>
            </div>
        </nav>
        <ul class="collapsible popout" data-collapsible="accordion">
            <li>
                <div class="collapsible-header"><i class="material-icons">place</i>Address</div>
                <div class="collapsible-body"><span><p>7151 S. 420 E. Midvale, Utah 84047</p></span></div>
            </li>
            <li>
                <div class="collapsible-header"><i class="material-icons">phone</i>Phone</div>
                <div class="collapsible-body"><span><p>Office: (801)671-5782</p></span></div>
            </li>
            <li>
                <div class="collapsible-header"><i class="material-icons">email</i>Email</div>
                <div class="collapsible-body"><span><p>Peter.Johson@pjwebdev.com</p></span></div>
            </li>
        </ul>
      </div>      
        <div class="row">
            <form class="col s8 offset-s2">
            <div class="row">
                <div class="input-field col s6">
                <input id="first_name" type="text" class="validate">
                <label for="first_name">First Name</label>
                </div>
                <div class="input-field col s6">
                <input id="last_name" type="text" class="validate">
                <label for="last_name">Last Name</label>
                </div>
            </div>
            <div>
              <div class="input-field col s6">
                <select class="browser-default">
                <option value="" disabled selected>Interested Course</option>
                <option value="1">CSS Masters</option>
                <option value="2">JS Ninja</option>
                <option value="3">HTML Kings</option>
                <option value="4">Open Source Everything</option>
                </select>
            </div>
            <div class="row">
                <div class="col s6">
                <div class="input-field inline">
                    <input id="email" type="email" class="validate">
                    <label for="email" data-error="wrong" data-success="right">Email</label>
                </div>
                </div>
            </div>
            </form>
            <div>
                <button class="btn waves-effect waves-light" type="submit" name="action">Submit
                    <i class="material-icons right">send</i>
                </button>
            </div>
        </div>
